  <h1>Login</h1>

  <% form_for :user do |f| %>
    <%= f.error_messages %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

      <%= f.submit 'Login' %> <%= link_to 'Register', new_user_path %>

  <% end %>
